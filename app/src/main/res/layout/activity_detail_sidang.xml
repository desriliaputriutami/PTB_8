<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".detail_Sidang_Activity">

    <Button
        android:id="@+id/button_back_detail_Sidang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:backgroundTint="@color/white"
        android:onClick="onbutton_back_detail_SidangClick"
        android:text="Back"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#000000"
        android:fontFamily="@font/poppinssemibold"
        android:text="Detail Sidang"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.07999998" />

    <TextView
        android:id="@+id/nama_mahasiswa"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppinssemibold"
        android:text="Adi Satria"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <TextView
        android:id="@+id/nim"
        android:layout_width="73dp"
        android:layout_height="24dp"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/poppinsregular"
        android:text="2011521049"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nama_mahasiswa"
        app:layout_constraintTop_toBottomOf="@+id/nama_mahasiswa" />

    <TextView
        android:id="@+id/judul_tugas_akhir"
        android:layout_width="94dp"
        android:layout_height="33dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/poppinsregular"
        android:text="Judul Tugas Akhir"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/isi_judul_tugas_akhir"
        android:layout_width="202dp"
        android:layout_height="59dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="4dp"
        android:fontFamily="@font/poppinsregular"
        android:text=": Sistem Pengaturan Pencahayaan Ruangan Berbasis Pada Rumah Pintar"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/judul_tugas_akhir"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/isi_dosen_pembimbing"
        android:layout_width="200dp"
        android:layout_height="34dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="6dp"
        android:fontFamily="@font/poppinsregular"
        android:text=": Syafril Adi, S.Kom."
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/isi_judul_tugas_akhir"
        app:layout_constraintTop_toBottomOf="@+id/judul_tugas_akhir" />

    <TextView
        android:id="@+id/isi_penguji"
        android:layout_width="208dp"
        android:layout_height="35dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="5dp"
        android:fontFamily="@font/poppinsregular"
        android:text=": Fitra Ade"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/isi_dosen_pembimbing"
        app:layout_constraintTop_toBottomOf="@+id/dosen_pembimbing" />

    <TextView
        android:id="@+id/isi_ruang_seminar"
        android:layout_width="179dp"
        android:layout_height="27dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppinsregular"
        android:text=": Ruang Seminar"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/isi_penguji"
        app:layout_constraintTop_toBottomOf="@+id/penguji"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/dosen_pembimbing"
        android:layout_width="110dp"
        android:layout_height="31dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppinsregular"
        android:text="Dosen Pembimbing"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/judul_tugas_akhir"
        app:layout_constraintTop_toBottomOf="@+id/judul_tugas_akhir" />

    <TextView
        android:id="@+id/penguji"
        android:layout_width="107dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppinsregular"
        android:text="Penguji  "
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/dosen_pembimbing"
        app:layout_constraintTop_toBottomOf="@+id/dosen_pembimbing" />

    <TextView
        android:id="@+id/lokasi"
        android:layout_width="99dp"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppinsregular"
        android:text="Lokasi"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/penguji"
        app:layout_constraintTop_toBottomOf="@+id/penguji" />


    <TextView
        android:id="@+id/lokasi2"
        android:layout_width="178dp"
        android:layout_height="28dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppinsregular"
        android:text="Jumat, 12 Desember 2021"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/isi_ruang_seminar"
        app:layout_constraintTop_toBottomOf="@+id/lokasi" />

    <TextView
        android:id="@+id/Jadwal"
        android:layout_width="99dp"
        android:layout_height="27dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppinsregular"
        android:text="Jadwal "
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/lokasi"
        app:layout_constraintTop_toBottomOf="@+id/lokasi" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="66dp"
        android:layout_height="61dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="200dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toTopOf="@+id/judul_tugas_akhir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.305"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/profile" />

    <Button
        android:id="@+id/buttonListPesertaSeminar"
        android:layout_width="327dp"
        android:layout_height="wrap_content"

        android:layout_marginTop="140dp"
        android:layout_marginBottom="5dp"
        android:backgroundTint="@color/main"
        android:text="List Peserta Seminar"
        android:onClick="onbuttonListPesertaSeminarClick"
        app:layout_constraintBottom_toTopOf="@+id/buttonListLogbook"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Jadwal" />

    <Button
        android:id="@+id/buttonListLogbook"
        android:layout_width="327dp"
        android:layout_height="48dp"

        android:layout_marginTop="10dp"
        android:layout_marginBottom="51dp"
        android:backgroundTint="@color/main"
        android:onClick="onbuttonListLogbookClick"
        android:text="List Logbook"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonListPesertaSeminar" />

</androidx.constraintlayout.widget.ConstraintLayout>